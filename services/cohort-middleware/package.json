{
  "name": "@tymlez/cohort-middleware",
  "version": "1.0.0",
  "description": "Cohort Middleware.",
  "scripts": {
    "dev:up": "./tools/bin/dev-up.sh",
    "clean": "rm -rf dist coverage",
    "build": "tsc --build --force",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "tymlez-cli client-tools bootstrap validate --filePath ./tools/lib/deploy/bootstrap.js",
    "test": "jest",
    "test:e2e": "jest --config ./jest-e2e.config.js",
    "deploy:cohort": "yarn build && yarn tools deploy",
    "tools": "./tools/bin/index.ts",
    "put-secrets": "tymlez-cli client-tools bootstrap put-secrets --bootstrapFilePath ./tools/lib/deploy/bootstrap.js --secretsFilePath ./tools/lib/deploy/bootstrap-secrets.js"
  },
  "dependencies": {
    "@eropple/nestjs-correlation-id": "^1.0.1",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^8.4.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/swagger": "^5.2.1",
    "@tymlez/backend-libs": "*",
    "@tymlez/common-libs": "*",
    "@tymlez/tymlez-cli": "*",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.28.0",
    "express": "^4.18.1",
    "lodash": "^4.17.21",
    "nestjs-pino": "^2.5.2",
    "p-retry": "4.6.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pino-http": "^6.6.0",
    "pino-pretty": "^7.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@tymlez/cohort-api-interfaces": "*",
    "@tymlez/platform-api-interfaces": "*",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.33",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@types/webpack-node-externals": "^2.5.3",
    "@types/yargs": "^17.0.10",
    "aws-sdk": "^2.1126.0",
    "exec-sh": "^0.4.0",
    "jest": "^27.5.1",
    "p-limit": "3.1.0",
    "prettier": "^2.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.4",
    "webpack-node-externals": "^3.0.0",
    "yargs": "^17.4.1"
  }
}
